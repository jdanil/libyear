{
  "name": "libyear",
  "version": "0.5.0",
  "description": "A simple measure of software dependency freshness",
  "author": "Joshua David <@jdanil>",
  "contributors": [
    "Jared Beck <@jaredbeck>",
    "Leon Miller-Out <@sbleon>"
  ],
  "license": "LGPL-3.0",
  "homepage": "https://github.com/jdanil/libyear",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdanil/libyear.git"
  },
  "bugs": {
    "url": "https://github.com/jdanil/libyear/issues"
  },
  "main": "./src",
  "bin": "./bin/libyear.js",
  "types": "./src",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.js"
  ],
  "keywords": [
    "debt",
    "dependency",
    "drift",
    "fitness",
    "libyear",
    "outdated"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "audit-ci": "audit-ci",
    "clean": "tsc --build --clean",
    "compile": "tsc --build",
    "fmt": "prettier --ignore-path .gitignore",
    "fmt:all": "yarn fmt .",
    "fmt:diff": "pretty-quick",
    "libyear": "ts-node ./bin/libyear",
    "lint": "eslint",
    "lint:all": "yarn lint \"**/*\"",
    "lint:diff": "eslint-changeset",
    "prepack": "tsc --build --force",
    "test": "jest --config .jestrc.json --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:precommit": "yarn test --bail --findRelatedTests",
    "test:pull-request": "yarn test:ci --changedSince=\"master\""
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "cosmiconfig": "^6.0.0",
    "date-fns": "^2.12.0",
    "execa": "^4.0.0",
    "fromentries": "^1.2.0",
    "lodash": "^4.17.15",
    "mri": "^1.1.5",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@jest/globals": "^26.0.0",
    "@types/lodash": "^4.14.150",
    "@types/mri": "^1.1.0",
    "@types/node": "^13.13.4",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "audit-ci": "^2.5.1",
    "eslint": "^6.8.0",
    "eslint-changeset": "^0.1.2",
    "husky": "^4.2.5",
    "jest": "^25.5.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
