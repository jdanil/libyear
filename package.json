{
  "name": "libyear",
  "version": "0.6.0",
  "description": "A simple measure of software dependency freshness",
  "author": "Joshua David <@jdanil>",
  "contributors": [
    "Jared Beck <@jaredbeck>",
    "Leon Miller-Out <@sbleon>"
  ],
  "license": "LGPL-3.0",
  "homepage": "https://github.com/jdanil/libyear",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdanil/libyear.git"
  },
  "bugs": {
    "url": "https://github.com/jdanil/libyear/issues"
  },
  "main": "./src",
  "bin": "./bin/libyear.js",
  "types": "./src",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.js"
  ],
  "keywords": [
    "debt",
    "dependency",
    "drift",
    "fitness",
    "libyear",
    "outdated"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "audit-ci": "audit-ci --config .auditcirc.json",
    "clean": "tsc --build --clean",
    "compile": "tsc --build",
    "fmt": "prettier --ignore-path .gitignore",
    "fmt:all": "yarn fmt .",
    "fmt:diff": "pretty-quick",
    "libyear": "ts-node ./bin/libyear",
    "lint": "eslint",
    "lint:all": "yarn lint \"{bin,src}/**/*\"",
    "lint:diff": "eslint-changeset",
    "prepack": "tsc --build --force",
    "test": "jest --config .jestrc.json --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:precommit": "yarn test --bail --findRelatedTests",
    "test:pull-request": "yarn test:ci --changedSince=\"master\""
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cosmiconfig": "^6.0.0",
    "date-fns": "^2.16.1",
    "execa": "^4.0.3",
    "fromentries": "^1.2.1",
    "lodash": "^4.17.20",
    "mri": "^1.1.6",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jest/globals": "^26.5.2",
    "@types/lodash": "^4.14.161",
    "@types/mri": "^1.1.0",
    "@types/node": "^14.11.5",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "audit-ci": "^3.1.1",
    "eslint": "^7.10.0",
    "eslint-changeset": "^0.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "jest-circus": "^26.5.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.2",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "jest-changed-files": "~26.1.0"
  }
}
