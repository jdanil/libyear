{
  "name": "libyear",
  "version": "0.7.0",
  "description": "A simple measure of software dependency freshness",
  "author": "Joshua David <@jdanil>",
  "contributors": [
    "Jared Beck <@jaredbeck>",
    "Leon Miller-Out <@sbleon>"
  ],
  "license": "LGPL-3.0",
  "homepage": "https://github.com/jdanil/libyear",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdanil/libyear.git"
  },
  "bugs": {
    "url": "https://github.com/jdanil/libyear/issues"
  },
  "main": "./src",
  "bin": "./bin/libyear.js",
  "types": "./src",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.js"
  ],
  "keywords": [
    "debt",
    "dependency",
    "drift",
    "fitness",
    "libyear",
    "outdated"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "audit-ci": "audit-ci --config .auditcirc.json",
    "clean": "tsc --build --clean",
    "compile": "tsc --build",
    "format": "prettier",
    "format:all": "yarn format .",
    "format:diff": "pretty-quick",
    "libyear": "ts-node ./bin/libyear",
    "lint": "eslint --cache",
    "lint:all": "yarn lint \"**/*\"",
    "lint:diff": "eslint-changeset",
    "prepack": "tsc --build --force",
    "prepare": "is-ci || husky install",
    "test": "jest --config .jestrc.json --passWithNoTests",
    "test:ci": "yarn test --ci",
    "test:coverage": "yarn test --coverage",
    "test:precommit": "yarn test --bail --findRelatedTests",
    "test:pull-request": "yarn test:ci --changedSince=\"master\""
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cosmiconfig": "^7.0.1",
    "date-fns": "^2.25.0",
    "execa": "^6.0.0",
    "lodash": "^4.17.21",
    "mri": "^1.2.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@jest/globals": "^27.3.1",
    "@types/lodash": "^4.14.177",
    "@types/node": "^17.0.23",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "audit-ci": "^6.1.2",
    "eslint": "^8.2.0",
    "eslint-changeset": "^0.1.5",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
